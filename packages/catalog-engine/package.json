{
  "name": "@unilab/catalog-engine",
  "version": "1.0.0",
  "description": "AI-powered product catalog generation engine for manufacturing SMEs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "catalog",
    "ai",
    "pdf-generation",
    "manufacturing",
    "openai",
    "vision-ai"
  ],
  "author": "uniLAB",
  "license": "MIT",
  "peerDependencies": {
    "openai": "^6.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "@react-pdf/renderer": "^4.3.2",
    "culori": "^4.0.2"
  },
  "devDependencies": {
    "@types/react": "^19",
    "tsup": "^8.0.1",
    "typescript": "^5"
  }
}
